{
  "name": "pydocstring",
  "rules": {
    "docstring": {
      "type": "CHOICE",
      "members": [
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SYMBOL",
              "name": "_headfootspace"
            },
            {
              "type": "BLANK"
            }
          ]
        },
        {
          "type": "SEQ",
          "members": [
            {
              "type": "SYMBOL",
              "name": "_headfootspace"
            },
            {
              "type": "SYMBOL",
              "name": "docstring_content"
            },
            {
              "type": "CHOICE",
              "members": [
                {
                  "type": "SYMBOL",
                  "name": "_headfootspace"
                },
                {
                  "type": "BLANK"
                }
              ]
            }
          ]
        }
      ]
    },
    "docstring_content": {
      "type": "PATTERN",
      "value": "[^\\n]*\\n?"
    },
    "_headfootspace": {
      "type": "PATTERN",
      "value": "\\s*"
    }
  },
  "extras": [],
  "conflicts": [],
  "precedences": [],
  "externals": [],
  "inline": [],
  "supertypes": []
}
